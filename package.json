{
  "name": "react-tokyo-website",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "waku dev",
    "build": "waku build --with-vercel-static",
    "start": "waku start",
    "generateGatheredBlogs": "tsx scripts/generateGatheredBlogs.ts",
